#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User1/OneDrive/Desktop/Dev Projects/bombbomb/bb-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/User1/OneDrive/Desktop/Dev Projects/bombbomb/bb-api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nopt.js" "$@"
else
  exec node  "$basedir/../../bin/nopt.js" "$@"
fi
